<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:system="clr-namespace:System;assembly=mscorlib">

    <system:String x:Key="Username">Username:</system:String>
    <system:String x:Key="Password">Password:</system:String>

    <system:String x:Key="Weight">Weight</system:String>
    <system:String x:Key="Arm">Arm</system:String>
    <system:String x:Key="Chest">Chest</system:String>
    <system:String x:Key="Hips">Hips</system:String>
    <system:String x:Key="Thigh">Thigh</system:String>
</ResourceDictionary>